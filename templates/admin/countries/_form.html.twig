<section class="w-100 p-4 d-flex justify-content-center pb-4 border mb-8">
        <div style="width: 26rem;">
          <!-- Pills navs -->
          <ul class="nav nav-pills nav-justified mb-3" id="ex1" role="tablist">
            <li class="nav-item" role="presentation">
              <a class="nav-link active" id="tab-login" data-mdb-toggle="pill" href="#pills-login" role="tab" aria-controls="pills-login" aria-selected="true">Country</a>
            </li>
          </ul>
          <!-- Pills navs -->

          <!-- Pills content -->
          <div class="tab-content">
           
            <div class="tab-pane fade show active" id="pills-login" role="tabpanel" aria-labelledby="tab-login">
             	{{ form_start(countrieForm) }}
               
                	<div class="form-outline mb-4">
						<input type="text" class="form-control" id="{{countrieForm.name.vars.full_name }}" name="{{ countrieForm.name.vars.full_name }}" placeholder="Country" value={{ countrieForm.name.vars.value }} autocomplete="firstname">
						{% do countrieForm.name.setRendered %}
						<label class="form-label" for="registerName">country</label>
					</div>


                <!-- Submit button -->
               <button type="submit" class="btn btn-primary btn-block mb-3">{{ button_label|default('Ajouter') }}</button>
              {{ form_end(countrieForm) }}
             
            </div>
          </div>
          <!-- Pills content -->
        </div>
      </section>


   
  

   

    
